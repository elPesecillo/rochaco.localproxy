{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-useless-catch": "off",
    "func-names": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "quotes": "off",
    "implicit-arrow-linebreak": "off",
    "function-paren-newline": "off",
    "comma-dangle": "off",
    "no-confusing-arrow": "off",
    "operator-linebreak": "off"
  }
}
